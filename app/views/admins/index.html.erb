<%= render 'application/top' %>
<%= render 'adminmenu' %>
<div id="main">
  <div class="breadcrumb-wrapper">
    <div class="container">
      <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12 row ">
        <h3 class="title">管理员</h3>
        <br>
        <br>
      <table class="table table-hover table-bordered">
        <thead>
        <tr>
          <th>用户名</th>
          <th>账号</th>
          <th>操作</th>
        </tr>
        </thead>
        <tbody>
        <% @admins.each do |admin| %>
            <tr>
              <td>
                管理员
              </td>
              <td>
                <%= admin.user_name %>
              </td>
              <td>
              <%= link_to '编辑',edit_admin_path(admin),class:"icon-pencil",'data-no-turbolink' => true %>&nbsp;
              </td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <br>
    </div>

  </div>
</div>
</div>

<%= render 'dibu' %>
