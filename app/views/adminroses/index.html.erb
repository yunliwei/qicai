<%= render 'form'%>
<div id="main">
  <div class="breadcrumb-wrapper">
    <div class="container">

      <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12 row ">
        <h3 class="title">玫瑰花作用与功效</h3>
        <table class="table table-striped table-bordered">
          <thead>
          <tr>
            <th>标题</th>
            <th>创建时间</th>
            <th>内容</th>
            <th>操作</th>
          </tr>
          </thead>
          <tbody>
          <%@adminrose.each do |adminrose| %>
              <tr>
                <td><%= adminrose.title %></td>
                <td>
                </td>
                <td><%= adminrose.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
                <td><%= adminrose.content%></td>
                <td><%= link_to '',edit_adminroses_path(adminrose),class:"icon-pencil",'data-no-turbolink' => true %>&nbsp;<%= link_to '', adminrose, method: :delete, data: { confirm: 'Are you sure?' },class:"icon-trash" %></td>
              </tr>
          <% end %>

          </tbody>
        </table>



        <div >

          <%= will_paginate @adminrose, previous_label:'上一页', next_label: '下一页' %>
        </div>
        <%= link_to '创建文章',new_adminroses_path,class:"btn-small btn-color btn-pad pull-right",'data-no-turbolink' => true %>
      </div>


    </div>
  </div>
</div>