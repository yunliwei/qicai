
<%= render 'top' %>

<%= render 'adminmenu' %>
<br>
<br>
<div id="main">
  <div class="breadcrumb-wrapper">
    <div class="container">
      <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12 row ">
        <h3 class="title">在线客服</h3>
        <br>
        <br>
        <table class="table table-striped table-bordered">
          <thead>
          <tr>
            <th>客服QQ</th>
            <th>客服电话</th>
            <th>创建时间</th>
            <th>操作</th>
          </tr>
          </thead>
          <tbody>
          <%@adminkefus.each do |f| %>
              <tr>
                <td><%= f.kefuqq %></td>
                <td><%= f.kefutel%></td>
                <td><%= f.created_at.strftime("%Y-%m-%d %H:%M:%S") %></td>
                <td><%= link_to '编辑',edit_adminkefu_path(f),class:"icon-pencil",'data-no-turbolink' => true %>&nbsp;<%= link_to '删除',f, method: :delete, data: { confirm: 'Are you sure?' },class:"icon-trash" %></td>
              </tr>
          <% end %>
          </tbody>
        </table>
        <%= link_to '创建',new_adminkefu_path,class: "btn btn-w-m btn-success mylink pull-right",'data-no-turbolink'=>true%>
      </div>
    </div>
  </div>
</div>
<%= render 'dibu' %>



